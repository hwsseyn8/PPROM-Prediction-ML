# PPROM Prediction Study - Exploratory Data Analysis

import sys
sys.path.append('../src')

import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
from src.config import Config
from src.data_preprocessing import DataPreprocessor

# Initialize
config = Config()
preprocessor = DataPreprocessor(config)

# Load data
df = preprocessor.load_data()

# Basic statistics
print("Dataset shape:", df.shape)
print("\nColumn types:")
print(df.dtypes.value_counts())

# Missing values
missing = df.isnull().sum()
print("\nMissing values per column:")
print(missing[missing > 0].sort_values(ascending=False))

# Outcome distribution
if config.primary_outcome in df.columns:
    outcome_counts = df[config.primary_outcome].value_counts()
    print(f"\nOutcome '{config.primary_outcome}' distribution:")
    print(outcome_counts)
    
    plt.figure(figsize=(8, 6))
    outcome_counts.plot(kind='bar')
    plt.title('Outcome Distribution')
    plt.xlabel('Class')
    plt.ylabel('Count')
    plt.xticks(rotation=0)
    plt.show()

# Correlation analysis
numeric_cols = df.select_dtypes(include=[np.number]).columns
if len(numeric_cols) > 0:
    corr_matrix = df[numeric_cols].corr()
    
    plt.figure(figsize=(12, 10))
    sns.heatmap(corr_matrix, cmap='coolwarm', center=0, square=True)
    plt.title('Correlation Matrix')
    plt.tight_layout()
    plt.show()

# Feature distributions
for col in df.columns[:10]:  # First 10 columns
    if df[col].dtype in [np.float64, np.int64]:
        plt.figure(figsize=(10, 4))
        
        plt.subplot(1, 2, 1)
        df[col].hist(bins=30)
        plt.title(f'{col} - Histogram')
        
        plt.subplot(1, 2, 2)
        df[col].plot(kind='box')
        plt.title(f'{col} - Boxplot')
        
        plt.tight_layout()
        plt.show()
